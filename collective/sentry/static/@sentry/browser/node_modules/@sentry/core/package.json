    "node": ">=6"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@sentry/hub": "4.0.0-beta.12",
    "@sentry/minimal": "4.0.0-beta.12",
    "@sentry/types": "4.0.0-beta.12",
    "@sentry/utils": "4.0.0-beta.12"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.2",
