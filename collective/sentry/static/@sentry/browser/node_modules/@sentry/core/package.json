{
  "name": "@sentry/core",
  "version": "4.0.0-beta.12",
  "description": "Base implementation for all Sentry JavaScript SDKs",
  "repository": {
    "type": "git",
    "url": "git://github.com/getsentry/raven-js.git"
  },
